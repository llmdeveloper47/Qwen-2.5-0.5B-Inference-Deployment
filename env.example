QUANTIZATION=none

# Load Testing Configuration
LOCUST_USERS=10
LOCUST_SPAWN_RATE=2
LOCUST_RUN_TIME=5m
BATCH_SIZE=8

# GitHub Container Registry
GITHUB_TOKEN=your-github-personal-access-token
